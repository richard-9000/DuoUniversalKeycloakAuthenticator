<settings xmlns="http://maven.apache.org/SETTINGS/1.1.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.1.0 http://maven.apache.org/xsd/settings-1.1.0.xsd">
  <servers>
    <server>
      <id>gitlab-maven</id>
      <configuration>
        <httpHeaders>
          <property>
            <name>Job-Token</name>
            <value>${env.CI_JOB_TOKEN}</value>
          </property>
        </httpHeaders>
      </configuration>
    </server>
    <server>
      <id>gitlab-maven-duosecurity</id>
      <configuration>
        <httpHeaders>
          <property>
            <name>Job-Token</name>
            <value>fxmyGUjqB1MN6g9Fesqp</value>
          </property>
        </httpHeaders>
      </configuration>
    </server>
  </servers>
  <profiles>
    <profile>
      <id>gitlab-maven</id>
      <activeByDefault>true</activeByDefault>
      <repositories>
        <repository>
            <id>gitlab-maven</id>
            <url>${env.CI_API_V4_URL}/projects/${env.CI_PROJECT_ID}/packages/maven</url>
        </repository>
        <repository>
          <id>gitlab-maven-duosecurity</id>
          <url>${env.CI_API_V4_URL}/projects/11/packages/maven</url>
        </repository>
      </repositories>
    </profile>
  </profiles>
</settings>
